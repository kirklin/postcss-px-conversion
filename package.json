  "repository": {
    "type": "git",
    "url": "git+https://github.com/kirklin/postcss-px-conversion.git"
  },
  "bugs": "https://github.com/kirklin/postcss-px-conversion/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
